@{
    string TitleTag = " | Goodfellows Electrical";

    if (!string.IsNullOrEmpty($"{ViewData["Title"]}"))
        ViewData["Title"] = ViewData["Title"].ToString().Substring(0, ViewData["Title"].ToString().Length > (60 - TitleTag.Length) ? (60 - TitleTag.Length) : ViewData["Title"].ToString().Length);
    else
        ViewData["Title"] = "Brisbane's Trusted Electricians"; // title is null, set title

    if (ViewData["Title"].ToString().Length == (60 - TitleTag.Length)) // Maxed out title, add ..
        ViewData["Title"] += "..";

    ViewData["Title"] += TitleTag;

    if (ViewData["Description"] != null)
        ViewData["Description"] = ViewData["Description"].ToString().Substring(0, ViewData["Description"].ToString().Length > 160 ? 160 : ViewData["Description"].ToString().Length);
    else
        ViewData["Description"] = "Brisbane's Trusted Electricians";

    if (ViewData["OgType"] == null)
        ViewData["OgType"] = "website";

    string domainName = Context.Request.Host.Value.ToLower();

    bool IsDotCom = domainName == "goodfellowselectrical.com" || domainName == "www.goodfellowselectrical.com";
    bool IsDotComAu = domainName == "goodfellowselectrical.com.au" || domainName == "www.goodfellowselectrical.com.au";

    TimeSpan currentTime = DateTime.Now.TimeOfDay;
    TimeSpan businessStart = TimeSpan.Parse("09:00");
    TimeSpan businessEnd = TimeSpan.Parse("17:00");

    bool withinBusinessHours = businessStart <= currentTime;
}

